{
  "title": "ERM Metrics",
  "schemaVersion": 16,
  "version": 0,
  "panels": [
    {
      "type": "graph",
      "title": "HTTP p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le,route))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "OSRM errors",
      "targets": [{ "expr": "rate(osrm_errors_total[5m])" }]
    },
    {
      "type": "graph",
      "title": "Rate limit drops",
      "targets": [{ "expr": "sum(rate(rate_limit_drops_total[5m])) by (name)" }]
    }
  ]
}
